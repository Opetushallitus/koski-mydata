  "main": "index.js",
  "jest": {
    "modulePathIgnorePatterns": [
      "dist/*"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "src/test/smoke/"
    ],
    "collectCoverageFrom": [
      "src/**"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/test/smoke/"
    ],
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "./jest-config.js"
  },
  "scripts": {
    "lint": "eslint src",
    "lint-tests": "eslint __tests__",
    "test": "jest --coverage",
    "build": "npm run test && npm run lint && gulp",
    "deploy": "aws lambda update-function-code --function-name $FUNCTION_NAME --zip-file fileb://dist.zip",
    "clean": "gulp clean",
    "start": "nodemon --watch src --watch docs --exec gulp js env docs & sam local start-api",
    "smoketest": "gulp js && node dist/test/smoke/ApiTest.js"
